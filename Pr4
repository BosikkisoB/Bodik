#include <stdio.h>

int main()
{
   int x;
   int y = 0;

   // Запитуємо користувача про натуральне число x
   printf("Введіть натуральне число x: ");
   scanf("%d", &x);

   // Перебираємо всі можливі дільники від 1 до x-1
   for (int i = 1; i < x; i++)
   {
      // Перевіряємо, чи є i рівним дільником числа x, якщо так, то збільшуємо лічильник
      if (x % i == 0)
      {
         y++;
      }
   }

   // Виводимо результат програми
   printf("Кількість дільників числа %d: %d\n", x, y);

   return 0;
}

